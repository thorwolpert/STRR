{
  "name": "strr-base-web",
  "private": true,
  "type": "module",
  "version": "0.0.8",
  "scripts": {
    "build-check": "nuxt build",
    "build": "nuxt generate",
    "dev": "nuxt dev --port 3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .ts,.vue app --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest --dir tests/unit --passWithNoTests --coverage && cp ./coverage/clover.xml ./coverage.xml",
    "test:unit": "vitest --dir tests/unit",
    "test:unit:cov": "vitest run --dir tests/unit --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/image": "^1.7.0",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.49.1",
    "@testing-library/vue": "^8.1.0",
    "@types/luxon": "^3.4.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.56.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "happy-dom": "^14.12.3",
    "nuxt": "^3.15.0",
    "playwright-core": "^1.49.1",
    "sass": "^1.77.6",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@daxiom/nuxt-core-layer-test": "^0.0.24",
    "@vuepic/vue-datepicker": "^10.0.0",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "country-codes-list": "^1.6.11",
    "luxon": "^3.5.0",
    "nuxt-gtag": "^3.0.2",
    "vue-country-flag-next": "^2.3.2"
  },
  "pnpm": {
    "overrides": {
      "vite": "5.4.11"
    }
  }
